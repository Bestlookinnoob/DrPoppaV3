	function GetNearestPlayerToMouse()
		local L_136_ = {}
		local L_137_ = {}
		local L_138_ = {}
		for L_140_forvar1, L_141_forvar2 in next, L_2_["Players"]:GetPlayers() do
			if L_141_forvar2 ~= L_4_["LocalPlayer"] then
				table.insert(L_136_, L_141_forvar2)
			end
		end
		for L_142_forvar1, L_143_forvar2 in next, L_136_ do
			local L_144_ = L_8_func(L_143_forvar2.Character, "Humanoid", "Find")
			local L_145_ = L_8_func(L_143_forvar2.Character, "Torso", "Find")
			if L_143_forvar2.Character ~= nil and L_145_ ~= nil and L_144_ ~= nil and L_144_.Health > 0 then
				local L_146_ = (L_145_.Position - L_4_["Camera"].CoordinateFrame.p).magnitude
				local L_147_ = Ray.new(L_4_["Camera"].CoordinateFrame.p, (L_4_["LocalPlayer"]:GetMouse().Hit.p - L_4_["Camera"].CoordinateFrame.p).unit * L_146_)
				local L_148_, L_149_ = workspace:FindPartOnRay(L_147_, workspace)
				local L_150_ = math.floor((L_149_ - L_145_.Position).magnitude)
				L_137_[L_143_forvar2.Name .. L_142_forvar1] = {}
				L_137_[L_143_forvar2.Name .. L_142_forvar1].dist = L_146_
				L_137_[L_143_forvar2.Name .. L_142_forvar1].plr = L_143_forvar2
				L_137_[L_143_forvar2.Name .. L_142_forvar1].diff = L_150_
				table.insert(L_138_, L_150_)
			end
		end
        
		local L_139_ = math.floor(math.min(unpack(L_138_)))
		if L_139_ > 20 then
			return
		end
        
		for L_151_forvar1, L_152_forvar2 in pairs(L_137_) do
			if L_152_forvar2.diff == L_139_ then
				return L_152_forvar2.plr
			end
		end
		return
	end

	local function L_82_func(L_153_arg1)
