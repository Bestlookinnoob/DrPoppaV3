						local L_203_ = L_56_func(L_201_, "DrPoppaBodyGyro", "Find")
						local L_204_ = L_56_func(L_201_, "DrPoppaBodyVelocity", "Find")
						if L_203_ ~= nil then
							L_203_:Destroy()
						end
						if L_204_ ~= nil then
							L_204_:Destroy()
						end
					end
				end
			end)()
			if L_6_["NoClip"] then
