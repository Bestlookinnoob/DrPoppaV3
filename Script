						L_303_.Velocity = L_304_
						L_302_.CFrame = L_4_["Camera"].CoordinateFrame
						L_299_.PlatformStand = false
						local L_305_ = L_299_.StateChanged:Connect(function(L_306_arg1, L_307_arg2)
							if L_307_arg2 == Enum.HumanoidStateType.PlatformStanding then
								L_299_.PlatformStand = false
							end
						end)
						L_299_:ChangeState(3)
						for L_308_forvar1, L_309_forvar2 in next, L_299_:GetPlayingAnimationTracks() do
							if string.find(tostring(L_309_forvar2.Animation.AnimationId), "125750702") then 
								L_309_forvar2:Stop()
							end
						end
						L_4_["RenderStepped"]:Wait()
						if L_299_ ~= nil then
							L_299_:ChangeState(8)
						end
						L_305_:Disconnect()
					end
				elseif not L_6_["Flying"] then
					local L_310_ = L_8_func(L_4_["LocalPlayer"].Character, "Torso", "Find")
					local L_311_ = L_8_func(L_4_["LocalPlayer"].Character, "Head", "Find")
					if L_310_ ~= nil and L_311_ ~= nil then
						L_311_.CanCollide = true
						L_310_.CanCollide = true
						local L_312_ = L_8_func(L_310_, "DrPoppaBodyGyro", "Find")
						local L_313_ = L_8_func(L_310_, "DrPoppaBodyVelocity", "Find")
						if L_312_ ~= nil then
							L_312_:Destroy()
						end
						if L_313_ ~= nil then
							L_313_:Destroy()
						end
					end
				end
			end)()
			if L_6_["NoClip"] then
				local L_314_ = L_8_func(L_4_["LocalPlayer"].Character, "Head", "Find")
				local L_315_ = L_8_func(L_4_["LocalPlayer"].Character, "Torso", "Find")
				if L_314_ ~= nil and L_315_ ~= nil then
					L_314_.CanCollide = false
					L_315_.CanCollide = false
				end
			end
			if L_6_["Aimbot"] and L_36_.Text == "" then
				L_6_["AimbotTarget"] = GetNearestPlayerToMouse()
			end
			if L_6_["AimbotTarget"] ~= nil then
				for L_316_forvar1, L_317_forvar2 in next, L_2_["Players"]:GetPlayers() do
					local L_318_ = L_8_func(L_317_forvar2.Character, "Torso", "Find")
					local L_319_ = L_8_func(L_317_forvar2.Character, "Torso", "Find")
					if L_319_ ~= nil and L_318_ ~= nil then
						local L_320_ = L_8_func(L_318_, "BoxHandleAdornment", "Find")
						if L_320_ ~= nil and L_317_forvar2 ~= L_6_["AimbotTarget"] then
							L_320_:Destroy()
						elseif L_317_forvar2 == L_6_["AimbotTarget"] and not L_8_func(L_318_, "BoxHandleAdornment", "Find") then
							local L_321_ = (L_319_.Position - L_318_.Position).magnitude / 25
							local L_322_ = Instance.new("BoxHandleAdornment", L_318_)
							L_322_.Adornee = L_318_
							L_322_.AlwaysOnTop = true
							L_322_.ZIndex = 5
							L_322_.Transparency = 0.5
							L_322_.Size = L_318_.Size * 2 + Vector3.new(L_321_, L_321_, L_321_)
							L_322_.Color3 = Color3.fromRGB(196, 40, 28)
							L_2_["Debris"]:AddItem(L_322_, 1)
						end
					end
				end
			end
		end
	end)()

	L_2_["Players"]:Chat("AA4AA")
	L_79_func()
	L_75_func("DrPoppa v3 Loaded, took " .. tick() - L_1_, "Really black")
	L_75_func("Press ' for the command bar & enter 'cmds' for the commands", "Really black")
	L_75_func("Join the discord if you want to suggest something, discord.gg/7KXNCPr", "Really black")
	L_75_func("Latest update, Van Enabled/Disabler for The Streets", "Really black")
end
