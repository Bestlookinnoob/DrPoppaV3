	L_2_["UserInputService"].InputBegan:Connect(function(L_167_arg1, L_168_arg2)
		if L_167_arg1.KeyCode == Enum.KeyCode.LeftShift and L_6_["Sprint"] then
			L_5_["LeftShift"] = true
		end
		if not L_168_arg2 then
			if L_167_arg1.KeyCode == Enum.KeyCode.Quote then
				L_53_.Size = UDim2.new(0, 0, 0, 25)
				L_53_.Position = UDim2.new(1, 0, 1, -295)
				L_53_.Visible = true
