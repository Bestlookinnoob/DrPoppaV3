			elseif L_162_arg1.KeyCode == Enum.KeyCode.LeftControl and L_5_["Airwalk"] then
				L_5_["Airwalk_Offset"] = -0.5
			elseif L_162_arg1.KeyCode == Enum.KeyCode.Space and L_5_["Airwalk"] then
				L_5_["Airwalk_Offset"] = 0.5
			elseif L_162_arg1.KeyCode == Enum.KeyCode.W then
				L_4_["W"] = true
			elseif L_162_arg1.KeyCode == Enum.KeyCode.A then
				L_4_["A"] = true
			elseif L_162_arg1.KeyCode == Enum.KeyCode.S then
				L_4_["S"] = true
			elseif L_162_arg1.KeyCode == Enum.KeyCode.D then
				L_4_["D"] = true
			elseif L_162_arg1.KeyCode == Enum.KeyCode.Q then
				L_4_["Q"] = true
			elseif L_162_arg1.KeyCode == Enum.KeyCode.E then
				L_4_["E"] = true
			elseif L_6_["NoClipHotkey"] ~= " " and L_162_arg1.KeyCode == Enum.KeyCode[L_6_["NoClipHotkey"]] then
				if not L_5_["NoClip"] then
					L_5_["NoClip"] = true
