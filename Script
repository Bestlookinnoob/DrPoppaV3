					L_55_func("You've been attacked by " .. tostring(L_86_arg1.Value))
				end
			end)
		end
	end

	local function L_58_func(L_87_arg1)
		local L_88_ = {}
		if L_87_arg1 == "all" then
			for L_89_forvar1, L_90_forvar2 in next, L_2_["Players"]:GetPlayers() do
				if L_90_forvar2 ~= L_3_["LocalPlayer"] then
					table.insert(L_88_, L_90_forvar2)
				end
			end
			return L_88_
		else
			for L_91_forvar1, L_92_forvar2 in next, L_2_["Players"]:GetPlayers() do
				if string.lower(tostring(L_87_arg1)) == string.sub(string.lower(tostring(L_92_forvar2)), 1, string.len(tostring(L_87_arg1))) then
					table.insert(L_88_, L_92_forvar2)
					return L_88_
				end
			end
		end
	end

	local function L_59_func()
		if writefile ~= nil and readfile ~= nil then
			writefile("DrPoppav3Settings.lua", L_2_["HttpService"]:JSONEncode(L_6_))
		else
			L_55_func("Exploit unsupportive of 'writefile' and 'readfile'")
		end
	end

	local function L_60_func(L_93_arg1)
		local L_94_ = L_54_func(L_3_["LocalPlayer"].Character, "Right Arm", "Find")
