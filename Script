	local L_66_ = debug.getmetatable(game)
	local L_67_ = L_66_.__newindex
	local L_68_ = L_66_.__namecall
	setreadonly(L_66_, false)

	L_66_.__newindex = newcclosure(function(L_172_arg1, L_173_arg2, L_174_arg3)
		if checkcaller() then 
			return L_67_(L_172_arg1, L_173_arg2, L_174_arg3)
		end
		if L_172_arg1 and L_172_arg1:IsA("Humanoid") then
			if L_173_arg2 == "Health" and L_174_arg3 == 0 then
				return
			end
			if L_173_arg2 == "WalkSpeed" and L_174_arg3 == 0 and L_6_["NoSlow"] then
				return
			end
		end
		if L_173_arg2 == "CFrame" and tostring(L_172_arg1) == "Torso" or tostring(L_172_arg1) == "HumanoidRootPart" then 
			return
		end
		return L_67_(L_172_arg1, L_173_arg2, L_174_arg3)
	end)

	L_66_.__namecall = newcclosure(function(L_175_arg1, ...)
		local L_176_ = {
			...
		}
		local L_177_ = getnamecallmethod()
		if not checkcaller() and L_177_ == "BreakJoints" then 
			return wait(9e9)
		end
		if L_177_ == "FireServer" then 
			if tostring(L_175_arg1) == "lIII" or tostring(L_175_arg1.Parent) == "ReplicatedStorage" then 
				return wait(9e9)
			end
			if tostring(L_175_arg1) == "Fire" then
				if L_6_["Aimbot"] and L_6_["AimbotTarget"] ~= nil then
					pcall(function()
						L_176_[1] = L_6_["AimbotTarget"].Character.Torso.CFrame + Vector3.new(L_6_["AimbotTarget"].Character.Torso.Velocity.X / L_7_["AimbotVelocity"], 0, L_6_["AimbotTarget"].Character.Torso.Velocity.Z / L_7_["AimbotVelocity"])
					end)
					return L_68_(L_175_arg1, unpack(L_176_))
				end
			end
			if tostring(L_175_arg1) == "Input" then
				if L_6_["Aimbot"] and L_6_["AimbotTarget"] ~= nil then
					pcall(function()
						L_176_[2]["mousehit"] = L_6_["AimbotTarget"].Character.Torso.CFrame + Vector3.new(L_6_["AimbotTarget"].Character.Torso.Velocity.X / L_7_["AimbotVelocity"], 0, L_6_["AimbotTarget"].Character.Torso.Velocity.Z / L_7_["AimbotVelocity"])
					end)
					return L_68_(L_175_arg1, unpack(L_176_))
				end
			end
			if L_176_[1] == "hey" then 
				return wait(9e9)
			end
		end
		return L_68_(L_175_arg1, ...)
	end)


	if L_4_["LocalPlayer"].Character ~= nil then
		L_59_func()
	end

