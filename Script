			until L_6_["CanNotify"] and #L_55_:GetChildren() < 6
			L_6_["CanNotify"] = false
			for L_82_forvar1, L_83_forvar2 in next, L_55_:GetChildren() do
				L_83_forvar2:TweenPosition(L_83_forvar2.Position + UDim2.new(0, 0, 0, -40), "Out", "Quad", 0.2, false)
				L_4_["RenderStepped"]:wait()
			end
			wait(0.2)
			local L_81_ = Instance.new("TextLabel", L_55_)
			L_81_.Size = UDim2.new(0, 185, 0, 35)
			L_81_.Position = UDim2.new(0.5, -92.5, 1, -40)
