				L_74_func()
			elseif L_6_["TpBypass"] then
				L_6_["TpBypass"] = false
				L_31_.BackgroundColor = BrickColor.new("Really red")
				L_74_func()
			end
		elseif L_165_[1] == "rejoin" then
			L_2_["TeleportService"]:Teleport(game.PlaceId, L_4_["LocalPlayer"])
		elseif L_165_[1] == "fly" then
			if not L_6_["Flying"] then
				L_6_["Flying"] = true
				L_21_.BackgroundColor = BrickColor.new("Lime green")
			elseif L_6_["Flying"] then
				L_6_["Flying"] = false
				L_21_.BackgroundColor = BrickColor.new("Really red")
			end
