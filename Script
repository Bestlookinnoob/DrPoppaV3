	function GetNearestPlayerToMouse()
		local L_134_ = {}
		local L_135_ = {}
		local L_136_ = {}
		for L_138_forvar1, L_139_forvar2 in next, L_2_["Players"]:GetPlayers() do
			if L_139_forvar2 ~= L_4_["LocalPlayer"] then
				table.insert(L_134_, L_139_forvar2)
			end
		end
		for L_140_forvar1, L_141_forvar2 in next, L_134_ do
			local L_142_ = L_71_func(L_141_forvar2.Character, "Humanoid", "Find")
			local L_143_ = L_71_func(L_141_forvar2.Character, "Torso", "Find")
			if L_141_forvar2.Character ~= nil and L_143_ ~= nil and L_142_ ~= nil and L_142_.Health > 0 then
				local L_144_ = (L_143_.Position - L_4_["Camera"].CoordinateFrame.p).magnitude
				local L_145_ = Ray.new(L_4_["Camera"].CoordinateFrame.p, (L_4_["LocalPlayer"]:GetMouse().Hit.p - L_4_["Camera"].CoordinateFrame.p).unit * L_144_)
				local L_146_, L_147_ = workspace:FindPartOnRay(L_145_, workspace)
				local L_148_ = math.floor((L_147_ - L_143_.Position).magnitude)
				L_135_[L_141_forvar2.Name .. L_140_forvar1] = {}
				L_135_[L_141_forvar2.Name .. L_140_forvar1].dist = L_144_
				L_135_[L_141_forvar2.Name .. L_140_forvar1].plr = L_141_forvar2
				L_135_[L_141_forvar2.Name .. L_140_forvar1].diff = L_148_
				table.insert(L_136_, L_148_)
			end
		end
        
		local L_137_ = math.floor(math.min(unpack(L_136_)))
		if L_137_ > 20 then
			return
		end
        
		for L_149_forvar1, L_150_forvar2 in pairs(L_135_) do
			if L_150_forvar2.diff == L_137_ then
				return L_150_forvar2.plr
			end
		end
		return
	end

