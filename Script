		end
	end

	local function L_61_func(L_95_arg1)
		if L_3_["LocalPlayer"].Character ~= nil and L_95_arg1.Character ~= nil then
			L_3_["LocalPlayer"].Character:BreakJoints()
			L_56_func()
			L_3_["LocalPlayer"].CharacterAdded:Wait()
			L_3_["RenderStepped"]:Wait()
			if L_3_["LocalPlayer"].Character ~= nil and L_95_arg1 ~= nil and L_95_arg1.Character ~= nil then
				local L_96_ = L_54_func(L_3_["LocalPlayer"].Character, "Humanoid", "Wait")
				local L_97_ = L_54_func(L_3_["LocalPlayer"].Character, "HumanoidRootPart", "Wait")
				local L_98_ = L_54_func(L_95_arg1.Character, "Torso", "Find")
				local L_99_ = L_54_func(L_95_arg1.Character, "Humanoid", "Find")
				if L_96_ ~= nil and L_97_ ~= nil and L_98_ ~= nil and L_99_ ~= nil and L_99_.Health ~= 0 then
					L_96_:UnequipTools()
					local L_100_ = L_54_func(L_3_["LocalPlayer"].Backpack, "Tool", "Find")
					if L_100_ ~= nil then
						L_96_.Name = ""
						local L_101_ = L_96_:Clone()
						L_101_.Parent = L_3_["LocalPlayer"].Character
						L_101_.Name = L_101_.ClassName
						L_96_:Destroy()
						L_101_.DisplayDistanceType = "None"
						L_3_["Camera"].CameraSubject = L_3_["LocalPlayer"].Character
						wait(0.1)
						if L_100_ ~= nil and L_3_["LocalPlayer"].Character ~= nil then
							L_100_.Parent = L_3_["LocalPlayer"].Character
							repeat
								L_97_.CFrame = L_98_.CFrame + L_98_.CFrame.rightVector * -2
								L_3_["RenderStepped"]:wait()
							until L_95_arg1.Character == nil or L_3_["LocalPlayer"].Character == nil or L_99_ == nil or L_99_.Health == 0 or L_96_.Health == 0 or L_100_.Parent == L_95_arg1.Character
							L_101_.Health = 0
							L_3_["LocalPlayer"].Character = nil
						end
					end
				end
			end
		end
	end

	function GetNearestPlayerToMouse()
		local L_102_ = {}
		local L_103_ = {}
		local L_104_ = {}
		for L_106_forvar1, L_107_forvar2 in next, L_2_["Players"]:GetPlayers() do
			if L_107_forvar2 ~= L_3_["LocalPlayer"] then
				table.insert(L_102_, L_107_forvar2)
			end
		end
		for L_108_forvar1, L_109_forvar2 in next, L_102_ do
			local L_110_ = L_54_func(L_109_forvar2.Character, "Humanoid", "Find")
			local L_111_ = L_54_func(L_109_forvar2.Character, "Torso", "Find")
			if L_109_forvar2.Character ~= nil and L_111_ ~= nil and L_110_ ~= nil and L_110_.Health > 0 then
				local L_112_ = (L_111_.Position - L_3_["Camera"].CoordinateFrame.p).magnitude
				local L_113_ = Ray.new(L_3_["Camera"].CoordinateFrame.p, (L_3_["LocalPlayer"]:GetMouse().Hit.p - L_3_["Camera"].CoordinateFrame.p).unit * L_112_)
				local L_114_, L_115_ = workspace:FindPartOnRay(L_113_, workspace)
				local L_116_ = math.floor((L_115_ - L_111_.Position).magnitude)
				L_103_[L_109_forvar2.Name .. L_108_forvar1] = {}
				L_103_[L_109_forvar2.Name .. L_108_forvar1].dist = L_112_
				L_103_[L_109_forvar2.Name .. L_108_forvar1].plr = L_109_forvar2
				L_103_[L_109_forvar2.Name .. L_108_forvar1].diff = L_116_
				table.insert(L_104_, L_116_)
			end
		end
        
		local L_105_ = math.floor(math.min(unpack(L_104_)))
		if L_105_ > 20 then
			return
		end
        
		for L_117_forvar1, L_118_forvar2 in pairs(L_103_) do
			if L_118_forvar2.diff == L_105_ then
				return L_118_forvar2.plr
			end
		end
		return
	end

	local function L_62_func(L_119_arg1)
		if L_119_arg1 ~= nil then
			local L_120_ = {}
			local L_121_ = L_54_func(L_3_["LocalPlayer"].Character, "Humanoid", "Find")
			local L_122_ = L_54_func(L_119_arg1.Character, "Humanoid", "Find")
			local L_123_ = L_54_func(L_119_arg1.Character, "Torso", "Find")
			if L_121_ ~= nil and L_121_.Health ~= 0 and L_122_ ~= nil and L_122_.Health ~= 0 and L_123_ ~= nil and not L_54_func(L_3_["LocalPlayer"].Character, "KO", "Find") and not L_54_func(L_119_arg1.Character, "KO", "Find") then
				L_121_:UnequipTools()
				for L_125_forvar1, L_126_forvar2 in next, L_3_["LocalPlayer"].Backpack:GetChildren() do
					if L_126_forvar2:IsA("Tool") and L_54_func(L_126_forvar2, "Ammo", "Find") and tostring(L_126_forvar2) == "Shotty" then
						table.insert(L_120_, L_126_forvar2)
					end
				end
				local L_124_ = L_54_func(L_3_["LocalPlayer"].Character, "Right Arm", "Find")
				if L_124_ ~= nil then
					for L_127_forvar1, L_128_forvar2 in next, L_120_ do
						local L_129_ = 0
						repeat
							L_129_ = L_129_ + 2
							local L_130_ = L_124_.CFrame * CFrame.new(0, -1, 0, 1, 0, 0, 0, 0, 1, 0, -1, 0)
							local L_131_ = L_130_:toObjectSpace(L_123_.CFrame * CFrame.Angles(0, math.rad(L_129_), math.rad(L_129_ + math.random(3))) * CFrame.new(0, 5, 5)):inverse()
							L_128_forvar2.Grip = L_131_
							L_128_forvar2.Parent = L_3_["LocalPlayer"].Character
							L_128_forvar2.Fire:FireServer(L_123_.Position)
							L_3_["RenderStepped"]:wait()
							L_128_forvar2.Parent = L_3_["LocalPlayer"].Backpack
						until L_128_forvar2.Clips.Value == 0 and L_120_.Ammo.Value == 0 or L_128_forvar2 == nil or not L_5_["Attacking"] or L_121_ == nil or L_121_.Health == 0 or L_122_ == nil or L_122_.Health == 0 or L_123_ == nil
					end
				end
			end
		end
	end

	local function L_63_func(L_132_arg1)
		local L_133_ = string.split(L_132_arg1, " ")
		for L_134_forvar1, L_135_forvar2 in next, L_133_ do
			L_135_forvar2 = string.lower(tostring(L_135_forvar2))
		end
		if L_133_[1] == "airwalk" then
			if not L_5_["Airwalk"] then
				L_5_["Airwalk"] = true
				L_15_.BackgroundColor = BrickColor.new("Lime green")
			elseif L_5_["Airwalk"] then
				L_5_["Airwalk"] = false
				L_15_.BackgroundColor = BrickColor.new("Really red")
			end
		elseif L_133_[1] == "rejoin" then
			L_2_["TeleportService"]:Teleport(game.PlaceId, L_3_["LocalPlayer"])
		elseif L_133_[1] == "fly" then
			if not L_5_["Flying"] then
				L_5_["Flying"] = true
				L_17_.BackgroundColor = BrickColor.new("Lime green")
			elseif L_5_["Flying"] then
				L_5_["Flying"] = false
				L_17_.BackgroundColor = BrickColor.new("Really red")
			end
		elseif L_133_[1] == "unfly" then
			L_5_["Flying"] = false
		elseif L_133_[1] == "flyspeed" then
			L_6_["FlySpeed"] = L_133_[2]
			L_55_func("Fly speed has been changed to " .. tostring(L_6_["FlySpeed"]))
			L_59_func()
		elseif L_133_[1] == "sprintspeed" then
			L_6_["SprintSpeed"] = L_133_[2]
			L_55_func("Sprint speed has been changed to " .. tostring(L_6_["SprintSpeed"]))
			L_5_["Sprint"] = true
			L_35_.Text = L_133_[2]
			L_34_.BackgroundColor = BrickColor.new("Lime green")
			L_59_func()
		elseif L_133_[1] == "camlock" then
			local L_136_ = L_58_func(L_133_[2])
			if L_136_ ~= nil then
				for L_137_forvar1, L_138_forvar2 in next, L_136_ do
					if L_138_forvar2 ~= L_3_["LocalPlayer"] then
						L_5_["Camlocking"] = true
						L_5_["CamlockTarget"] = L_138_forvar2
						L_31_.BackgroundColor = BrickColor.new("Lime green")
						L_32_.Text = tostring(L_138_forvar2)
					end
				end
			end
		elseif L_133_[1] == "attack" then
			local L_139_ = L_58_func(L_133_[2])
			if L_139_ ~= nil then
				for L_140_forvar1, L_141_forvar2 in next, L_139_ do
					if L_141_forvar2 ~= L_3_["LocalPlayer"] then
						L_5_["Attacking"] = true
						L_62_func(L_141_forvar2)
					end
				end
			end
		elseif L_133_[1] == "uncamlock" then
			L_5_["CamlockTarget"] = nil
			L_5_["Camlocking"] = false
			L_31_.BackgroundColor = BrickColor.new("Really red")
			L_32_.Text = ""
		elseif L_133_[1] == "view" then
			local L_142_ = L_58_func(L_133_[2])
			if L_142_ ~= nil then
				for L_143_forvar1, L_144_forvar2 in next, L_142_ do
					L_3_["Camera"].CameraSubject = L_144_forvar2.Character
				end
			end
		elseif L_133_[1] == "unview" then
			if L_3_["LocalPlayer"].Character ~= nil then
				L_3_["Camera"].CameraSubject = L_3_["LocalPlayer"].Character
			end
		elseif L_133_[1] == "re" or L_133_[1] == "reset" or L_133_[1] == "respawn" then
			L_56_func()
		elseif L_133_[1] == "kill" then
			local L_145_ = L_58_func(L_133_[2])
			if L_145_ ~= nil then
				for L_146_forvar1, L_147_forvar2 in next, L_145_ do
					L_61_func(L_147_forvar2)
				end
			end
		end
	end

