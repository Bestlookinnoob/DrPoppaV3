		L_3_["LocalPlayer"].Character = L_80_
	end

	local function L_57_func()
		if L_3_["LocalPlayer"].Character ~= nil then
			local L_84_ = L_54_func(L_3_["LocalPlayer"].Character, "Humanoid", "Wait")
