						L_6_["Camlocking"] = true
						L_6_["CamlockTarget"] = L_170_forvar2
						L_40_.BackgroundColor = BrickColor.new("Lime green")
						L_41_.Text = tostring(L_170_forvar2)
					end
				end
			end
		elseif L_165_[1] == "feloop" then
