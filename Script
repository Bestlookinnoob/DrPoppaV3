	local L_64_ = debug.getmetatable(game)
	local L_65_ = L_64_.__newindex
	local L_66_ = L_64_.__namecall
	setreadonly(L_64_, false)

	L_64_.__newindex = newcclosure(function(L_167_arg1, L_168_arg2, L_169_arg3)
		if checkcaller() then 
			return L_65_(L_167_arg1, L_168_arg2, L_169_arg3)
		end
		if L_167_arg1 and L_167_arg1:IsA("Humanoid") then
			if L_168_arg2 == "Health" and L_169_arg3 == 0 then
				return
			end
			if L_168_arg2 == "WalkSpeed" and L_169_arg3 == 0 and L_5_["NoSlow"] then
				return
			end
		end
		if L_168_arg2 == "CFrame" and tostring(L_167_arg1) == "Torso" or tostring(L_167_arg1) == "HumanoidRootPart" then 
			return
		end
		return L_65_(L_167_arg1, L_168_arg2, L_169_arg3)
	end)

	L_64_.__namecall = newcclosure(function(L_170_arg1, ...)
		local L_171_ = {
			...
		}
		local L_172_ = getnamecallmethod()
		if not checkcaller() and L_172_ == "BreakJoints" then 
			return wait(9e9)
		end
		if L_172_ == "FireServer" then 
			if tostring(L_170_arg1) == "lIII" or tostring(L_170_arg1.Parent) == "ReplicatedStorage" then 
				return wait(9e9)
			end
			if tostring(L_170_arg1) == "Fire" then
				if L_5_["Aimbot"] and L_5_["AimbotTarget"] ~= nil then
					pcall(function()
						L_171_[1] = L_5_["AimbotTarget"].Character.Torso.CFrame + Vector3.new(L_5_["AimbotTarget"].Character.Torso.Velocity.X / L_6_["AimbotVelocity"], 0, L_5_["AimbotTarget"].Character.Torso.Velocity.Z / L_6_["AimbotVelocity"])
					end)
					return L_66_(L_170_arg1, unpack(L_171_))
				end
			end
			if tostring(L_170_arg1) == "Input" then
				if L_5_["Aimbot"] and L_5_["AimbotTarget"] ~= nil then
					pcall(function()
						L_171_[2]["mousehit"] = L_5_["AimbotTarget"].Character.Torso.CFrame + Vector3.new(L_5_["AimbotTarget"].Character.Torso.Velocity.X / L_6_["AimbotVelocity"], 0, L_5_["AimbotTarget"].Character.Torso.Velocity.Z / L_6_["AimbotVelocity"])
					end)
					return L_66_(L_170_arg1, unpack(L_171_))
				end
			end
			if L_171_[1] == "hey" then 
				return wait(9e9)
			end
		end
		return L_66_(L_170_arg1, ...)
	end)


	if L_3_["LocalPlayer"].Character ~= nil then
		L_57_func()
	end

