						L_308_.CanCollide = true
						local L_310_ = L_71_func(L_308_, "DrPoppaBodyGyro", "Find")
						local L_311_ = L_71_func(L_308_, "DrPoppaBodyVelocity", "Find")
						if L_310_ ~= nil then
							L_310_:Destroy()
						end
						if L_311_ ~= nil then
							L_311_:Destroy()
						end
					end
				end
			end)()
			if L_6_["NoClip"] then
				local L_312_ = L_71_func(L_4_["LocalPlayer"].Character, "Head", "Find")
				local L_313_ = L_71_func(L_4_["LocalPlayer"].Character, "Torso", "Find")
				if L_312_ ~= nil and L_313_ ~= nil then
					L_312_.CanCollide = false
					L_313_.CanCollide = false
				end
			end
			if L_6_["Aimbot"] and L_35_.Text == "" then
				L_6_["AimbotTarget"] = GetNearestPlayerToMouse()
			end
			if L_6_["AimbotTarget"] ~= nil then
				for L_314_forvar1, L_315_forvar2 in next, L_2_["Players"]:GetPlayers() do
					local L_316_ = L_71_func(L_315_forvar2.Character, "Torso", "Find")
					local L_317_ = L_71_func(L_315_forvar2.Character, "Torso", "Find")
					if L_317_ ~= nil and L_316_ ~= nil then
						local L_318_ = L_71_func(L_316_, "BoxHandleAdornment", "Find")
						if L_318_ ~= nil and L_315_forvar2 ~= L_6_["AimbotTarget"] then
							L_318_:Destroy()
						elseif L_315_forvar2 == L_6_["AimbotTarget"] and not L_71_func(L_316_, "BoxHandleAdornment", "Find") then
							local L_319_ = (L_317_.Position - L_316_.Position).magnitude / 25
							local L_320_ = Instance.new("BoxHandleAdornment", L_316_)
							L_320_.Adornee = L_316_
							L_320_.AlwaysOnTop = true
							L_320_.ZIndex = 5
							L_320_.Transparency = 0.5
							L_320_.Size = L_316_.Size * 2 + Vector3.new(L_319_, L_319_, L_319_)
							L_320_.Color3 = Color3.fromRGB(196, 40, 28)
							L_2_["Debris"]:AddItem(L_320_, 1)
						end
					end
				end
			end
		end
	end)()

	L_2_["Players"]:Chat("AA4AA")
	L_77_func()
	L_73_func("DrPoppa v3 Loaded", "Really black")
	L_73_func("Took " .. tick() - L_1_, "Really black")
	L_73_func("Press ' for the command bar & enter 'cmds' for the commands", "Really black")
	L_73_func("Join the discord if you want to suggest something, discord.gg/7KXNCPr", "Really black")
end
