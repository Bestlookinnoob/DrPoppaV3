	local function L_56_func(L_75_arg1, L_76_arg2, L_77_arg3)
		if L_75_arg1 ~= nil and L_76_arg2 ~= nil then
			if L_77_arg3 == "Find" then
				local L_78_ = L_75_arg1:FindFirstChildOfClass(L_76_arg2)
				if not L_78_ then
					L_78_ = L_75_arg1:FindFirstChild(L_76_arg2)
				end
				if L_78_ then
					return L_78_
				end
			elseif L_77_arg3 == "Wait" then
				local L_79_ = L_75_arg1:WaitForChild(L_76_arg2)
				if L_79_ then
					return L_79_
				end
			end
		end
	end

	local function L_57_func(L_80_arg1)
		if L_6_["CanNotify"] then
