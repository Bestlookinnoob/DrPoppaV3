			for L_184_forvar1, L_185_forvar2 in next, L_2_["Players"]:GetPlayers() do
				local L_186_ = L_56_func(L_185_forvar2.Character, "Humanoid", "Find")
				if L_186_ ~= nil then
					L_186_.NameDisplayDistance = 9e9
					L_186_.HealthDisplayDistance = 9e9
				end
			end
			if L_6_["Airwalk"] then
				local L_187_ = L_56_func(workspace, "DrPoppaAirwalkPart", "Find")
				local L_188_ = L_56_func(L_4_["LocalPlayer"].Character, "HumanoidRootPart", "Find")
				if L_187_ == nil then
					L_187_ = Instance.new("Part", workspace)
					L_187_.Size = Vector3.new(5, 1, 5)
					L_187_.Anchored = true
					L_187_.Name = "DrPoppaAirwalkPart"
					L_187_.Transparency = 1
