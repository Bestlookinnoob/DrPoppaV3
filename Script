				end
			end)
		end
	end

	local function L_60_func(L_92_arg1)
		local L_93_ = {}
		if L_92_arg1 == "all" then
			for L_94_forvar1, L_95_forvar2 in next, L_2_["Players"]:GetPlayers() do
				if L_95_forvar2 ~= L_4_["LocalPlayer"] then
					table.insert(L_93_, L_95_forvar2)
				end
			end
			return L_93_
		else
			for L_96_forvar1, L_97_forvar2 in next, L_2_["Players"]:GetPlayers() do
				if string.lower(tostring(L_92_arg1)) == string.sub(string.lower(tostring(L_97_forvar2)), 1, string.len(tostring(L_92_arg1))) then
					table.insert(L_93_, L_97_forvar2)
					return L_93_
				end
			end
		end
	end

	local function L_61_func()
		if writefile ~= nil and readfile ~= nil then
			writefile("DrPoppav3Settings.lua", L_2_["HttpService"]:JSONEncode(L_7_))
		else
