						L_191_.PlatformStand = false
						local L_196_ = L_191_.StateChanged:Connect(function(L_197_arg1, L_198_arg2)
							if L_198_arg2 == Enum.HumanoidStateType.PlatformStanding then
								L_191_.PlatformStand = false
							end
						end)
						L_191_:ChangeState(3)
						for L_199_forvar1, L_200_forvar2 in next, L_191_:GetPlayingAnimationTracks() do
							if L_200_forvar2.Animation.AnimationId == "http://www.roblox.com/asset/?id=125750702" then 
								L_200_forvar2:Stop()
							end
						end
						L_4_["RenderStepped"]:Wait()
						if L_191_ ~= nil then
							L_191_:ChangeState(8)
						end
						if L_192_ ~= nil then
							L_192_.CFrame = CFrame.new(L_192_.Position, L_4_["Camera"].CoordinateFrame.lookVector * 9e9)
						end
						L_196_:Disconnect()
					end
				elseif not L_6_["Flying"] then
					local L_201_ = L_56_func(L_4_["LocalPlayer"].Character, "Torso", "Find")
					local L_202_ = L_56_func(L_4_["LocalPlayer"].Character, "Humanoid", "Find")
