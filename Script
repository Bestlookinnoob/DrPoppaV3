				if L_210_ ~= nil then
					for L_211_forvar1, L_212_forvar2 in next, L_210_ do
						if L_212_forvar2 ~= L_4_["LocalPlayer"] then
							local L_213_ = L_56_func(L_212_forvar2.Character, "Torso", "Find")
							if L_213_ ~= nil then
								L_4_["Camera"].CoordinateFrame = CFrame.new(L_4_["Camera"].CoordinateFrame.p, L_213_.Position)
							end
						end
					end
				elseif L_6_["CamlockTarget"] ~= nil and L_6_["Camlocking"] then
