    -- // LOCAL FUNCTIONS \\ --

	local function L_54_func(L_70_arg1, L_71_arg2, L_72_arg3)
		if L_70_arg1 ~= nil and L_71_arg2 ~= nil then
			if L_72_arg3 == "Find" then
				local L_73_ = L_70_arg1:FindFirstChildOfClass(L_71_arg2)
				if not L_73_ then
					L_73_ = L_70_arg1:FindFirstChild(L_71_arg2)
				end
				if L_73_ then
					return L_73_
				end
			elseif L_72_arg3 == "Wait" then
				local L_74_ = L_70_arg1:WaitForChild(L_71_arg2)
				if L_74_ then
					return L_74_
				end
			end
		end
	end

	local function L_55_func(L_75_arg1)
		if L_5_["CanNotify"] then
			repeat
				L_3_["RenderStepped"]:wait()
			until L_5_["CanNotify"] and #L_53_:GetChildren() < 6
			L_5_["CanNotify"] = false
			for L_77_forvar1, L_78_forvar2 in next, L_53_:GetChildren() do
				L_78_forvar2:TweenPosition(L_78_forvar2.Position + UDim2.new(0, 0, 0, -40), "Out", "Quad", 0.2, false)
				L_3_["RenderStepped"]:wait()
			end
			wait(0.2)
			local L_76_ = Instance.new("TextLabel", L_53_)
			L_76_.Size = UDim2.new(0, 185, 0, 35)
			L_76_.Position = UDim2.new(0.5, -92.5, 1, -40)
			L_76_.BackgroundColor = BrickColor.new("Salmon")
			L_76_.TextColor = BrickColor.new("Really black")
			L_76_.Font = "GothamBlack"
			L_76_.TextScaled = true
			L_76_.Transparency = 0.2
			L_76_.BorderSizePixel = 2
			L_76_.Text = L_75_arg1
			L_76_.BorderColor = BrickColor.new("White")
			spawn(function()
				wait(5)
				for L_79_forvar1 = 1, 16 do
					L_76_.Transparency = L_76_.Transparency + 0.1 / 2
					L_3_["RenderStepped"]:wait()
				end
				L_76_:Destroy()
			end)
			L_3_["RenderStepped"]:wait()
			L_5_["CanNotify"] = true
		end
	end

	local function L_56_func()
		L_3_["LocalPlayer"].Character:ClearAllChildren()
