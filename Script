						local L_193_ = L_56_func(L_190_, "DrPoppaBodyGyro", "Find")
						local L_194_ = L_56_func(L_190_, "DrPoppaBodyVelocity", "Find")
						if L_193_ == nil then
							L_193_ = Instance.new("BodyGyro", L_190_)
							L_193_.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
							L_193_.Name = "DrPoppaBodyGyro"
						end
						if L_194_ == nil then
							L_194_ = Instance.new("BodyVelocity", L_190_)
							L_194_.MaxForce = Vector3.new(9e9, 9e9, 9e9)
							L_194_.P = 9e4
							L_194_.Name = "DrPoppaBodyVelocity"
						end
						L_193_.CFrame = L_190_.CFrame
						local L_195_ = Vector3.new(0, 0, 0)
						if L_5_["W"] then
							L_195_ = L_195_ + L_192_.CFrame.lookVector * L_7_["FlySpeed"] * 25 
						end
						if L_5_["S"] then
							L_195_ = L_195_ + L_192_.CFrame.lookVector * -L_7_["FlySpeed"] * 25
						end
						if L_5_["A"] then
							L_195_ = L_195_ + L_192_.CFrame.rightVector * -L_7_["FlySpeed"] * 25
						end
						if L_5_["D"] then
							L_195_ = L_195_ + L_192_.CFrame.rightVector * L_7_["FlySpeed"] * 25
						end
						if L_5_["Q"] then
							L_195_ = L_195_ + Vector3.new(0, -L_7_["FlySpeed"] * 25, 0)
						end
						if L_5_["E"] then
							L_195_ = L_195_ + Vector3.new(0, L_7_["FlySpeed"] * 25, 0)
						end
						if not L_5_["W"] and not L_5_["S"] and not L_5_["A"] and not L_5_["D"] then
							L_195_ = Vector3.new(0, 0, 0)
						end
						L_194_.Velocity = L_195_
						if L_192_ ~= nil then
							L_192_.CFrame = CFrame.new(L_192_.Position, L_4_["Camera"].CoordinateFrame.lookVector * 9e9) + L_192_.CFrame.lookVector * 0.07
						end
