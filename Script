					L_23_.BackgroundColor = BrickColor.new("Really red")
				end
			elseif L_6_["FlyHotkey"] ~= " " and L_162_arg1.KeyCode == Enum.KeyCode[L_6_["FlyHotkey"]] then
				if not L_5_["Flying"] then
					L_5_["Flying"] = true
					L_17_.BackgroundColor = BrickColor.new("Lime green")
				elseif L_5_["Flying"] then
					L_5_["Flying"] = false
					L_17_.BackgroundColor = BrickColor.new("Really red")
				end
			elseif L_6_["AirwalkHotkey"] ~= " " and L_162_arg1.KeyCode == Enum.KeyCode[L_6_["AirwalkHotkey"]] then
				if not L_5_["Airwalk"] then
					L_5_["Airwalk"] = true
					L_15_.BackgroundColor = BrickColor.new("Lime green")
				elseif L_5_["Airwalk"] then
					L_5_["Airwalk"] = false
					L_15_.BackgroundColor = BrickColor.new("Really red")
				end
			elseif L_6_["ResetHotkey"] ~= " " and L_162_arg1.KeyCode == Enum.KeyCode[L_6_["ResetHotkey"]] then
				if L_3_["LocalPlayer"].Character ~= nil then
					L_3_["LocalPlayer"].Character:BreakJoints()
				end
			end
		end
	end)

