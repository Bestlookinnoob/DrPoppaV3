						until L_160_forvar2 == nil or L_160_forvar2.Clips.Value == 0 and L_152_.Ammo.Value == 0 or not L_6_["Attacking"] or L_153_ == nil or L_153_.Health == 0 or L_154_ == nil or L_154_.Health == 0 or L_155_ == nil
					end
				end
			end
		end
	end

	local function L_81_func(L_164_arg1)
		local L_165_ = string.split(L_164_arg1, " ")
		for L_166_forvar1, L_167_forvar2 in next, L_165_ do
			L_167_forvar2 = string.lower(tostring(L_167_forvar2))
		end
		if L_165_[1] == "airwalk" then
			if not L_6_["Airwalk"] then
				L_6_["Airwalk"] = true
				L_19_.BackgroundColor = BrickColor.new("Lime green")
			elseif L_6_["Airwalk"] then
				L_6_["Airwalk"] = false
				L_19_.BackgroundColor = BrickColor.new("Really red")
			end
		elseif L_165_[1] == "unairwalk" then
			L_6_["Airwalk"] = false
			L_19_.BackgroundColor = BrickColor.new("Really red")
		elseif L_165_[1] == "tpbypass" then
