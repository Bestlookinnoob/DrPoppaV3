						if L_189_ == nil then
							L_189_ = Instance.new("BodyVelocity", L_185_)
							L_189_.MaxForce = Vector3.new(9e9, 9e9, 9e9)
							L_189_.P = 9e4
							L_189_.Name = "DrPoppaBodyVelocity"
