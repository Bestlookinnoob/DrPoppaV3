	local L_84_ = debug.getmetatable(game)
	local L_85_ = L_84_.__newindex
	local L_86_ = L_84_.__namecall
	setreadonly(L_84_, false)

	L_84_.__newindex = newcclosure(function(L_235_arg1, L_236_arg2, L_237_arg3)
		if checkcaller() then 
			return L_85_(L_235_arg1, L_236_arg2, L_237_arg3)
		end
		if L_235_arg1 and L_235_arg1:IsA("Humanoid") then
			if L_236_arg2 == "Health" and L_237_arg3 == 0 then
				return
			end
			if L_236_arg2 == "WalkSpeed" and L_237_arg3 == 0 and L_6_["NoSlow"] then
				return
			end
		end
		if L_236_arg2 == "CFrame" and tostring(L_235_arg1) == "Torso" or tostring(L_235_arg1) == "HumanoidRootPart" then 
			return
		end
		return L_85_(L_235_arg1, L_236_arg2, L_237_arg3)
	end)

	L_84_.__namecall = newcclosure(function(L_238_arg1, ...)
		local L_239_ = {
			...
		}
		local L_240_ = getnamecallmethod()
		if not checkcaller() and L_240_ == "BreakJoints" then 
			return wait(9e9)
		end
		if L_240_ == "FireServer" then 
			if tostring(L_238_arg1) == "lIII" or tostring(L_238_arg1.Parent) == "ReplicatedStorage" then 
				return wait(9e9)
			end
			if tostring(L_238_arg1) == "Fire" then
