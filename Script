		end
	end

	local function L_63_func(L_100_arg1)
		if L_4_["LocalPlayer"].Character ~= nil and L_100_arg1.Character ~= nil then
			L_4_["LocalPlayer"].Character:BreakJoints()
			L_58_func()
			L_4_["LocalPlayer"].CharacterAdded:Wait()
			L_4_["RenderStepped"]:Wait()
			if L_4_["LocalPlayer"].Character ~= nil and L_100_arg1 ~= nil and L_100_arg1.Character ~= nil then
				local L_101_ = L_56_func(L_4_["LocalPlayer"].Character, "Humanoid", "Wait")
				local L_102_ = L_56_func(L_4_["LocalPlayer"].Character, "HumanoidRootPart", "Wait")
				local L_103_ = L_56_func(L_100_arg1.Character, "Torso", "Find")
				local L_104_ = L_56_func(L_100_arg1.Character, "Humanoid", "Find")
				if L_101_ ~= nil and L_102_ ~= nil and L_103_ ~= nil and L_104_ ~= nil and L_104_.Health ~= 0 then
					L_101_:UnequipTools()
					local L_105_ = L_56_func(L_4_["LocalPlayer"].Backpack, "Tool", "Find")
					if L_105_ ~= nil then
						L_101_.Name = ""
						local L_106_ = L_101_:Clone()
						L_106_.Parent = L_4_["LocalPlayer"].Character
						L_106_.Name = L_106_.ClassName
						L_101_:Destroy()
						L_106_.DisplayDistanceType = "None"
						L_4_["Camera"].CameraSubject = L_4_["LocalPlayer"].Character
						wait(0.1)
						if L_105_ ~= nil and L_4_["LocalPlayer"].Character ~= nil then
							L_105_.Parent = L_4_["LocalPlayer"].Character
							repeat
								L_102_.CFrame = L_103_.CFrame + L_103_.CFrame.rightVector * -2
								L_4_["RenderStepped"]:wait()
							until L_100_arg1.Character == nil or L_4_["LocalPlayer"].Character == nil or L_104_ == nil or L_104_.Health == 0 or L_101_.Health == 0 or L_105_.Parent == L_100_arg1.Character
							L_106_.Health = 0
							L_4_["LocalPlayer"].Character = nil
						end
					end
				end
			end
		end
	end

	function GetNearestPlayerToMouse()
		local L_107_ = {}
		local L_108_ = {}
		local L_109_ = {}
		for L_111_forvar1, L_112_forvar2 in next, L_2_["Players"]:GetPlayers() do
			if L_112_forvar2 ~= L_4_["LocalPlayer"] then
				table.insert(L_107_, L_112_forvar2)
			end
		end
		for L_113_forvar1, L_114_forvar2 in next, L_107_ do
			local L_115_ = L_56_func(L_114_forvar2.Character, "Humanoid", "Find")
			local L_116_ = L_56_func(L_114_forvar2.Character, "Torso", "Find")
			if L_114_forvar2.Character ~= nil and L_116_ ~= nil and L_115_ ~= nil and L_115_.Health > 0 then
				local L_117_ = (L_116_.Position - L_4_["Camera"].CoordinateFrame.p).magnitude
				local L_118_ = Ray.new(L_4_["Camera"].CoordinateFrame.p, (L_4_["LocalPlayer"]:GetMouse().Hit.p - L_4_["Camera"].CoordinateFrame.p).unit * L_117_)
				local L_119_, L_120_ = workspace:FindPartOnRay(L_118_, workspace)
				local L_121_ = math.floor((L_120_ - L_116_.Position).magnitude)
				L_108_[L_114_forvar2.Name .. L_113_forvar1] = {}
				L_108_[L_114_forvar2.Name .. L_113_forvar1].dist = L_117_
				L_108_[L_114_forvar2.Name .. L_113_forvar1].plr = L_114_forvar2
				L_108_[L_114_forvar2.Name .. L_113_forvar1].diff = L_121_
				table.insert(L_109_, L_121_)
			end
		end
        
		local L_110_ = math.floor(math.min(unpack(L_109_)))
		if L_110_ > 20 then
			return
		end
        
		for L_122_forvar1, L_123_forvar2 in pairs(L_108_) do
			if L_123_forvar2.diff == L_110_ then
				return L_123_forvar2.plr
			end
		end
		return
	end

	local function L_64_func(L_124_arg1)
		if L_124_arg1 ~= nil then
			local L_125_ = {}
			local L_126_ = L_56_func(L_4_["LocalPlayer"].Character, "Humanoid", "Find")
			local L_127_ = L_56_func(L_124_arg1.Character, "Humanoid", "Find")
			local L_128_ = L_56_func(L_124_arg1.Character, "Torso", "Find")
			if L_126_ ~= nil and L_126_.Health ~= 0 and L_127_ ~= nil and L_127_.Health ~= 0 and L_128_ ~= nil and not L_56_func(L_4_["LocalPlayer"].Character, "KO", "Find") and not L_56_func(L_124_arg1.Character, "KO", "Find") then
				L_126_:UnequipTools()
				for L_130_forvar1, L_131_forvar2 in next, L_4_["LocalPlayer"].Backpack:GetChildren() do
					if L_131_forvar2:IsA("Tool") and L_56_func(L_131_forvar2, "Ammo", "Find") and tostring(L_131_forvar2) == "Shotty" then
						table.insert(L_125_, L_131_forvar2)
					end
				end
				local L_129_ = L_56_func(L_4_["LocalPlayer"].Character, "Right Arm", "Find")
				if L_129_ ~= nil then
					for L_132_forvar1, L_133_forvar2 in next, L_125_ do
						local L_134_ = 0
						repeat
							L_134_ = L_134_ + 2
							local L_135_ = L_129_.CFrame * CFrame.new(0, -1, 0, 1, 0, 0, 0, 0, 1, 0, -1, 0)
							local L_136_ = L_135_:toObjectSpace(L_128_.CFrame * CFrame.Angles(0, math.rad(L_134_), math.rad(L_134_ + math.random(3))) * CFrame.new(0, 5, 5)):inverse()
							L_133_forvar2.Grip = L_136_
							L_133_forvar2.Parent = L_4_["LocalPlayer"].Character
							L_133_forvar2.Fire:FireServer(L_128_.Position)
							L_4_["RenderStepped"]:wait()
							L_133_forvar2.Parent = L_4_["LocalPlayer"].Backpack
						until L_133_forvar2.Clips.Value == 0 and L_125_.Ammo.Value == 0 or L_133_forvar2 == nil or not L_6_["Attacking"] or L_126_ == nil or L_126_.Health == 0 or L_127_ == nil or L_127_.Health == 0 or L_128_ == nil
					end
				end
			end
		end
	end

	local function L_65_func(L_137_arg1)
		local L_138_ = string.split(L_137_arg1, " ")
		for L_139_forvar1, L_140_forvar2 in next, L_138_ do
			L_140_forvar2 = string.lower(tostring(L_140_forvar2))
		end
		if L_138_[1] == "airwalk" then
			if not L_6_["Airwalk"] then
				L_6_["Airwalk"] = true
				L_16_.BackgroundColor = BrickColor.new("Lime green")
			elseif L_6_["Airwalk"] then
				L_6_["Airwalk"] = false
				L_16_.BackgroundColor = BrickColor.new("Really red")
			end
		elseif L_138_[1] == "rejoin" then
			L_2_["TeleportService"]:Teleport(game.PlaceId, L_4_["LocalPlayer"])
		elseif L_138_[1] == "fly" then
			if not L_6_["Flying"] then
				L_6_["Flying"] = true
				L_18_.BackgroundColor = BrickColor.new("Lime green")
			elseif L_6_["Flying"] then
				L_6_["Flying"] = false
				L_18_.BackgroundColor = BrickColor.new("Really red")
			end
		elseif L_138_[1] == "unfly" then
			L_6_["Flying"] = false
		elseif L_138_[1] == "flyspeed" then
			L_7_["FlySpeed"] = L_138_[2]
			L_57_func("Fly speed has been changed to " .. tostring(L_7_["FlySpeed"]))
			L_61_func()
		elseif L_138_[1] == "sprintspeed" then
			L_7_["SprintSpeed"] = L_138_[2]
			L_57_func("Sprint speed has been changed to " .. tostring(L_7_["SprintSpeed"]))
			L_6_["Sprint"] = true
			L_36_.Text = L_138_[2]
			L_35_.BackgroundColor = BrickColor.new("Lime green")
			L_61_func()
		elseif L_138_[1] == "camlock" then
			local L_141_ = L_60_func(L_138_[2])
			if L_141_ ~= nil then
				for L_142_forvar1, L_143_forvar2 in next, L_141_ do
					if L_143_forvar2 ~= L_4_["LocalPlayer"] then
						L_6_["Camlocking"] = true
						L_6_["CamlockTarget"] = L_143_forvar2
						L_32_.BackgroundColor = BrickColor.new("Lime green")
						L_33_.Text = tostring(L_143_forvar2)
					end
				end
			end
		elseif L_138_[1] == "attack" then
			local L_144_ = L_60_func(L_138_[2])
			if L_144_ ~= nil then
				for L_145_forvar1, L_146_forvar2 in next, L_144_ do
					if L_146_forvar2 ~= L_4_["LocalPlayer"] then
						L_6_["Attacking"] = true
						L_64_func(L_146_forvar2)
					end
				end
			end
		elseif L_138_[1] == "uncamlock" then
			L_6_["CamlockTarget"] = nil
			L_6_["Camlocking"] = false
			L_32_.BackgroundColor = BrickColor.new("Really red")
			L_33_.Text = ""
		elseif L_138_[1] == "view" then
			local L_147_ = L_60_func(L_138_[2])
			if L_147_ ~= nil then
				for L_148_forvar1, L_149_forvar2 in next, L_147_ do
					L_4_["Camera"].CameraSubject = L_149_forvar2.Character
				end
			end
		elseif L_138_[1] == "unview" then
			if L_4_["LocalPlayer"].Character ~= nil then
				L_4_["Camera"].CameraSubject = L_4_["LocalPlayer"].Character
			end
		elseif L_138_[1] == "re" or L_138_[1] == "reset" or L_138_[1] == "respawn" then
			L_58_func()
		elseif L_138_[1] == "cmds" then
			if not L_6_["CommandsOpen"] then
				L_54_.Visible = true
				L_6_["CommandsOpen"] = true
			elseif not L_6_["CommandsOpen"] then
				L_6_["CommandsOpen"] = false
				L_54_.Visible = false
			end
		elseif L_138_[1] == "kill" then
			local L_150_ = L_60_func(L_138_[2])
			if L_150_ ~= nil then
				for L_151_forvar1, L_152_forvar2 in next, L_150_ do
					L_63_func(L_152_forvar2)
				end
			end
		end
	end

