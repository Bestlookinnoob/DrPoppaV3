				local L_183_ = L_54_func(L_3_["LocalPlayer"].Character, "HumanoidRootPart", "Find")
				if L_182_ == nil then
					L_182_ = Instance.new("Part", workspace)
					L_182_.Size = Vector3.new(5, 1, 5)
					L_182_.Anchored = true
					L_182_.Name = "DrPoppaAirwalkPart"
					L_182_.Transparency = 1
				end
