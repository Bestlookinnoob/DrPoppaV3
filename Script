						local L_163_ = 0
						repeat
							if L_162_forvar2 ~= nil then
								L_163_ = L_163_ + 2
								local L_164_ = L_158_.CFrame * CFrame.new(0, -1, 0, 1, 0, 0, 0, 0, 1, 0, -1, 0)
								local L_165_ = L_164_:toObjectSpace(L_157_.CFrame * CFrame.Angles(0, math.rad(L_163_), math.rad(L_163_ + math.random(3))) * CFrame.new(0, 5, 5)):inverse()
								L_162_forvar2.Grip = L_165_
								L_162_forvar2.Parent = L_4_["LocalPlayer"].Character
								L_162_forvar2.Fire:FireServer(L_157_.Position)
								L_4_["RenderStepped"]:wait()
								L_162_forvar2.Parent = L_4_["LocalPlayer"].Backpack
							end
						until L_162_forvar2 == nil or L_162_forvar2.Clips.Value == 0 and L_154_.Ammo.Value == 0 or not L_6_["Attacking"] or L_155_ == nil or L_155_.Health == 0 or L_156_ == nil or L_156_.Health == 0 or L_157_ == nil
					end
				end
			end
		end
	end

	local function L_83_func(L_166_arg1)
		local L_167_ = string.split(L_166_arg1, " ")
		for L_168_forvar1, L_169_forvar2 in next, L_167_ do
