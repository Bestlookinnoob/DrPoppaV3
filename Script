	L_2_["UserInputService"].InputBegan:Connect(function(L_162_arg1, L_163_arg2)
		if L_162_arg1.KeyCode == Enum.KeyCode.LeftShift and L_5_["Sprint"] then
			L_4_["LeftShift"] = true
		end
		if not L_163_arg2 then
